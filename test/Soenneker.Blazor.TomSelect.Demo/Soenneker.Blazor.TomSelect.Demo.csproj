<?xml version="1.0" encoding="utf-8"?><Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
      <TargetFramework>net10.0</TargetFramework>
      <Nullable>enable</Nullable>
      <Platforms>AnyCPU</Platforms>
      <NoWarn>$(NoWarn);1591</NoWarn>
  <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild></PropertyGroup>

  <ItemGroup>
      <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="9.0.10" />
      <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="9.0.10" PrivateAssets="all" />
      <PackageReference Include="Soenneker.Blazor.Utils.Navigation" Version="3.0.1011" />
      <PackageReference Include="Soenneker.Serilog.Sinks.Browser.Blazor" Version="3.0.196" />
      <PackageReference Include="Soenneker.Utils.AutoBogus" Version="3.0.809" />
      <PackageReference Include="System.Net.Http.Json" Version="9.0.10" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\src\Soenneker.Blazor.TomSelect.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Content Update="wwwroot\sample-data\countries.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
